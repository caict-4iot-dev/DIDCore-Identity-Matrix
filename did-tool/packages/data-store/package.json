{
  "name": "data-store",
  "version": "1.0.0",
  "dependencies": {
    "typeorm": "^0.2.39",
    "sqlite3": "^5.0.2",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decentralized-identity/veramo.git",
    "directory": "packages/data-store"
  },
  "author": "fanzhimeng@bubi.cn",
  "keywords": [
    "bitfactory",
    "DID",
    "Verifiable Credential",
    "did-manager",
    "did-plugin"
  ],
  "license": "Apache-2.0",
  "type": "commonjs",
  "moduleDirectories": [
    "node_modules",
    "lib"
  ],
  "scripts": {
    "test": "mocha  -t 10000 packages/**/*.spec.js",
    "test didStore": "mocha  -t 10000 packages/**/didStore.spec.js",
    "test keyStore": "mocha  -t 10000 packages/**/keyStore.spec.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/_tests_/.*|(\\.|/)(test|spec))\\.js$",
    "moduleFileExtensions": ["js", "json", "node"]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0"
  }
}
