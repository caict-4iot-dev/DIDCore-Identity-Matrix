{
  "name": "did-manage",
  "version": "1.0.0",
  "dependencies": {
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.39",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decentralized-identity/veramo.git",
    "directory": "packages/data-store"
  },
  "author": "fanzhimeng@bubi.cn",
  "keywords": [
    "bitfactory",
    "DID",
    "Verifiable Credential",
    "did-manager",
    "did-plugin"
  ],
  "license": "Apache-2.0",
  "type": "commonjs",
  "moduleDirectories": [
    "node_modules",
    "lib"
  ],
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/_tests_/.*|(\\.|/)(test|spec))\\.js$",
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "jest": "^29.7.0"
  }
}
